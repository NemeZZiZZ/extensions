{
	"name": "collaborative-editing-bundle",
	"type": "module",
	"version": "1.0.0",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "hook",
				"name": "collaborative-editing-hook",
				"source": "src/server/hook.ts"
			},
			{
				"type": "interface",
				"name": "interface",
				"source": "src/interface/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"dependencies": {
		"@directus/composables": "^11.1.8",
		"@directus/constants": "^13.0.0",
		"@directus/sdk": "^19.0.1",
		"@directus/types": "^13.0.0",
		"@directus/utils": "^13.0.2",
		"@hocuspocus/extension-database": "^2.15.2",
		"@hocuspocus/provider": "^2.15.2",
		"@hocuspocus/server": "^2.15.2",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@tiptap/extension-collaboration": "^2.11.5",
		"@tiptap/extension-collaboration-cursor": "^2.11.5",
		"@tiptap/pm": "^2.11.5",
		"@tiptap/starter-kit": "^2.11.5",
		"@tiptap/vue-3": "^2.11.5",
		"@types/lodash-es": "^4.17.12",
		"@types/ws": "^8.5.10",
		"@vueuse/core": "^13.0.0",
		"colorjs.io": "^0.5.0",
		"cookie": "^1.0.2",
		"express": "^4.21.2",
		"express-ws": "^5.0.2",
		"js-base64": "^3.7.7",
		"jsonwebtoken": "^9.0.2",
		"lib0": "^0.2.94",
		"lodash-es": "^4.17.21",
		"vue-router": "4",
		"ws": "^8.18.1",
		"y-prosemirror": "^1.3.0",
		"y-protocols": "^1.0.6",
		"yjs": "^13.6.24"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.0.3",
		"@types/node": "^22.13.13",
		"typescript": "^5.8.2",
		"vue": "^3.5.13"
	}
}
